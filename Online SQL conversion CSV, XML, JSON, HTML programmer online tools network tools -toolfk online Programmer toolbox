STEP 2

? THE CORE CODE IS AS FOLLOWS

```
$ ( ". Sql-xml"). Click (function () {
    if (toolfk.beautify_default.beautify_in_progress) {
        return;
    }
    var source = sourceArea.getValue ();
    if (source == '') {
        return layer.msg (NOT_EMPTY);
    }
    var loading = layer.load ();
    toolfk.beautify_default.beautify_in_progress = true;
    sqlTOxml (source, targetArea);
    toolfk.beautify_default.beautify_in_progress = false;
    layer.close (loading);
});

$ ( ". Sql-csv"). Click (function () {

    if (toolfk.beautify_default.beautify_in_progress) {
        return;
    }
    var loading = layer.load ();
    var source = sourceArea.getValue ();
    if (source == '') {
        layer.close (loading);
        return layer.msg (NOT_EMPTY);
    }

    toolfk.beautify_default.beautify_in_progress = true;
    sqlTOcsv (source, targetArea);
    toolfk.beautify_default.beautify_in_progress = false;
    layer.close (loading);
});

$ ( ". Sql-json"). Click (function () {

    if (toolfk.beautify_default.beautify_in_progress) {
        return;
    }
    var loading = layer.load ();
    var source = sourceArea.getValue ();
    if (source == '') {
        layer.close (loading);
        return layer.msg (NOT_EMPTY);
    }

    toolfk.beautify_default.beautify_in_progress = true;
    sqlTOjson (source, targetArea);
    toolfk.beautify_default.beautify_in_progress = false;
    layer.close (loading);
});

$ ( ". Sql-yaml"). Click (function () {

    if (toolfk.beautify_default.beautify_in_progress) {
        return;
    }
    var loading = layer.load ();
    var source = sourceArea.getValue ();
    if (source == '') {
        layer.close (loading);
